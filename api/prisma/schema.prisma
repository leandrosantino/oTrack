generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Users {
  id          Int      @id @default(autoincrement())
  username    String   @unique
  password    String
  displayName String
  roule       String
  tokens      Tokens[]
}

model Tokens {
  id     Int   @id @default(autoincrement())
  userId Int
  user   Users @relation(fields: [userId], references: [id])
}
